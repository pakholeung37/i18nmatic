{
  "name": "@terminus/t-i18n-runtime",
  "version": "0.2.0",
  "description": "i18n runtime library for automating internationalization (i18n) code transformation and key extraction in React and Next.js projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "prebuild": "rm -rf dist",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "publish": "pnpm publish -r --registry=https://registry.npm.terminus.io/ --no-git-checks"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "react",
    "nextjs",
    "cli",
    "automation",
    "translation",
    "babel",
    "typescript"
  ],
  "author": "pakholeung",
  "license": "MIT",
  "dependencies": {
    "react-i18next": "^15.0.0",
    "i18next": "^25.0.0"
  },
  "peerDependencies": {
    "react": "*"
  }
}
