{
  "name": "@terminus/t-i18n-cli",
  "version": "0.2.0",
  "description": "CLI tool for automating internationalization (i18n) code transformation and key extraction in React and Next.js projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "bin": {
    "t-i18n-cli": "./bin/cli.js"
  },
  "scripts": {
    "dev": "tsc --watch",
    "start": "node bin/cli.js",
    "build": "tsc",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "prebuild": "rm -rf dist",
    "test:cov": "vitest --coverage",
    "publish": "pnpm publish --registry=https://registry.npm.terminus.io/ --no-git-checks"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "react",
    "nextjs",
    "cli",
    "automation",
    "translation",
    "babel",
    "typescript"
  ],
  "author": "pakholeung",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.26.9",
    "@babel/generator": "^7.26.9",
    "@babel/parser": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/traverse": "^7.26.9",
    "@babel/types": "^7.26.9",
    "commander": "^13.1.0",
    "glob": "^11.0.1",
    "lodash": "^4.17.21",
    "prettier": "^3.5.3"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.26.0",
    "@types/babel__traverse": "^7.20.6",
    "@types/lodash": "^4.17.20"
  }
}
