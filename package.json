{
  "name": "i18n-monorepo",
  "version": "1.0.7",
  "description": "CLI tool for automating internationalization (i18n) code transformation and key extraction in React and Next.js projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "bin",
    "auto-i18n.config.json",
    "README.md"
  ],
  "bin": {
    "auto-i18n": "./bin/cli.js"
  },
  "scripts": {
    "start": "node bin/cli.js",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "prebuild": "rm -rf dist",
    "test:cov": "vitest --coverage",
    "auto-i18n": "auto-i18n"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seonghunYang/i18nmatic.git"
  },
  "homepage": "https://github.com/seonghunYang/i18nmatic#readme",
  "bugs": {
    "url": "https://github.com/seonghunYang/i18nmatic/issues"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "react",
    "nextjs",
    "cli",
    "automation",
    "translation",
    "babel",
    "typescript"
  ],
  "author": "seonghunYang",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.26.9",
    "@babel/traverse": "^7.26.9",
    "@babel/types": "^7.26.9",
    "commander": "^13.1.0",
    "glob": "^11.0.1",
    "prettier": "^3.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/generator": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@types/babel__traverse": "^7.20.6",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/node": "^22.13.8",
    "@types/babel__generator": "^7.26.0",
    "vitest": "^3.2.0",
    "typescript": "^5.8.2"
  }
}
