    "@babel/preset-typescript": "^7.26.0",
    "@types/babel__traverse": "^7.20.6",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/node": "^22.13.8",
    "@types/babel__generator": "^7.26.0",
    "vitest": "^3.2.0",
    "typescript": "^5.8.2"
  }
