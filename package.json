{
  "name": "i18nmatic",
  "version": "1.0.0",
  "description": "CLI tool for automating internationalization (i18n) code transformation and key extraction in React and Next.js projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "auto-i18n": "./bin/cli.js"
  },
  "scripts": {
    "start": "node bin/cli.js",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "prebuild": "rm -rf dist",
    "test:cov": "jest --coverage",
    "auto-i18n": "auto-i18n"
  },
  "repository": {
    "type": "git",
    "url": "git@github-personal:seonghunYang/i18nmatic.git"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "react",
    "nextjs",
    "cli",
    "automation",
    "translation",
    "babel",
    "typescript"
  ],
  "author": "seonghunYang",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.26.9",
    "@babel/traverse": "^7.26.9",
    "@babel/types": "^7.26.9",
    "commander": "^13.1.0",
    "glob": "^11.0.1",
    "prettier": "^3.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/generator": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@types/babel__traverse": "^7.20.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.8",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
